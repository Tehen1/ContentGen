{
  "name": "@fixie-run/web3-utils",
  "version": "0.1.0",
  "description": "Web3 utilities and hooks for Fixie.Run blockchain integration",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "web3",
    "ethereum",
    "blockchain",
    "wagmi",
    "ethers",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fixie-run/types": "^0.1.0",
    "@tanstack/react-query": "^5.77.2",
    "ethers": "^6.11.1",
    "siwe": "^2.1.4",
    "viem": "^2.7.15",
    "wagmi": "^2.5.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
