{
  "name": "@fixie-run/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for Fixie.Run",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "types",
    "typescript",
    "blockchain",
    "fixie"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
