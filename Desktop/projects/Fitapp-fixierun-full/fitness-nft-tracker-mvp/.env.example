# Database Configuration
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/fitness_nft_db?schema=public"

# Server Configuration
PORT=3000
NODE_ENV=development

# Authentication
JWT_SECRET=your-super-secret-key-change-this-in-production
JWT_EXPIRES_IN=24h

# Blockchain/Web3 Configuration
ETHEREUM_NETWORK=goerli
WEB3_PROVIDER_URL=https://eth-goerli.g.alchemy.com/v2/your-api-key
NFT_CONTRACT_ADDRESS=0x...
WALLET_PRIVATE_KEY=your-private-key

# Docker PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=fitness_nft_db

# API Keys
ALCHEMY_API_KEY=your-alchemy-api-key
PINATA_API_KEY=your-pinata-api-key
PINATA_SECRET_KEY=your-pinata-secret-key
