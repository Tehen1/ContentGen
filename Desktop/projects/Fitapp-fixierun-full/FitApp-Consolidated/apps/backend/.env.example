# .env.example for Google Fitness API Flask Backend

# Google OAuth 2.0 Configuration
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
OAUTH_REDIRECT_URI=http://localhost:5000/oauth2callback

# Flask Secret Key (for sessions, CSRF)
FLASK_SECRET_KEY=your-super-secret-value

# (Optional) Database connection URI
DATABASE_URI=sqlite:///db.sqlite3

# (Optional) Prometheus metrics port
PROMETHEUS_METRICS_PORT=8000

